{
  "name": "klindosclient",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.jztnik.klindosclient",
    "productName": "KLIND OS Client Arch",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "electron": "^x.x.x",
    "electron-builder": "^24.6.3"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "node-fetch": "^2.7.0",
    "url": "^0.11.2"
  }
}
