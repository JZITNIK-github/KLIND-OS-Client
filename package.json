{
  "name": "klindosclient",
  "version": "1.0.0",
  "description": "KLIND OS Client je proram pro spuštění KLIND OS jako program",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JZITNIK-github/KLIND-OS-Client.git"
  },
  "author": "Jakub Žitník",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JZITNIK-github/KLIND-OS-Client/issues"
  },
  "homepage": "https://github.com/JZITNIK-github/KLIND-OS-Client#readme",
  "build": {
    "appId": "com.jztnik.klindosclient",
    "productName": "KLIND OS Client Arch",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "electron": "^x.x.x",
    "electron-builder": "^24.6.3"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "node-fetch": "^2.7.0",
    "url": "^0.11.2"
  }
}
